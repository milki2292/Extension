{
    "name" : "My extension", 
    "description" : "My first chrome ex",
    "version": "0.1",
    "author" : "Kamil Szmuc",
    "permissions" : ["contextMenus","activeTab"],
    "manifest_version" : 2,
    "background" : {
        "scripts" : ["src/background.js"]
    },
    "content_scripts" : [
        {
            "matches" : ["*://www.google.com/*", "*://www.youtube.com/*"],
            "js" : ["src/index.js"]
        }
    ]
}